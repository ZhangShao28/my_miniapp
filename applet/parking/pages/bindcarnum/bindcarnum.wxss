.li_list .li_item_content,.li_list .li_item_contents {
  position: static;
  margin-top: 0;
  opacity: 0;
  display: none;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
  -webkit-transition: 0.3s;
  transition: 0.3s;
}
 .li_list .li_item_show {
  position: static;
  margin-top: 0;
  opacity: 1;
  display: block;
  -webkit-transform: translateY(0);
  transform: translateY(0);
  -webkit-transition: 0.3s;
  transition: 0.3s;
}
 


